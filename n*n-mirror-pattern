// prog.2 : 1-dash-no-pattern.cpp
#include <iostream>
using namespace std;

int main() {
int i,j,n; 
cin>>n;
for(i=0;i<n;i++)
    {
    	for(j=0;j<n;j++)
            {
        	if(i<=j)
                cout <<i+1<< "*";
    	    if(j==i) 
              	cout <<i+1;
            }
       	for(j=0;j<n;j++)
            {
            	if(j<=i)
                    cout << "*";
            }
            cout<<endl;
   }
  return 0;
}
